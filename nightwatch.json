{
  "src_folders": ["./tests"],

  "webdriver": {
    "start_process": true,
     "server_path": "/usr/local/lib/node_modules/chromedriver/bin/chromedriver",
    "cli_args": [ "--verbose"],
    "port": 9515
  },

  "test_settings" : {
    "default" : {
      "desiredCapabilities" : {
        "browserName": "chrome"
      },
      "end_session_on_fail" : false,
      "skip_testcases_on_fail" : false
    }
  }
}
